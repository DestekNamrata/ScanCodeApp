<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/rel_forgot"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            android:background="@drawable/bg"
            >
            <ImageView
                android:id="@+id/img_bckScanDet"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginTop="@dimen/margin15"
                android:src="@drawable/ic_arrow_back"/>
            <com.vayortricks.vtproductscanner.CustomView.CustomTextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Scan Details"
                app:customFont="@string/str_font_bold"
                android:padding="@dimen/margin10"
                android:textColor="@color/white"
                android:textAllCaps="true"
                android:fontFamily="sans-serif"
                android:layout_marginTop="10dp"
                android:gravity="center"
                android:textSize="@dimen/textSize24"/>


        </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_margin="@dimen/margin15"
            >

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:layout_marginTop="@dimen/margin35"

                android:src="@drawable/logo"/>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="200dp"
                android:layout_marginBottom="10dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                app:cardCornerRadius="@dimen/margin15"
                >
                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:fillViewport="true">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/margin10"
                        android:orientation="vertical"
                        android:layout_margin="@dimen/margin10"

                        android:gravity="center_horizontal"
                        android:layout_centerInParent="true"
                        >
                        <ImageView
                            android:id="@+id/img_scanImg"
                            android:layout_width="180dp"
                            android:layout_height="180dp"
                            android:layout_marginTop="@dimen/margin15"
                            android:src="@drawable/ic_vt_logo"
                            android:scaleType="fitXY"
                            />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="25dp"
                            >
                            <com.vayortricks.vtproductscanner.CustomView.CustomTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Format Type :"
                                android:textStyle="bold"
                                android:layout_weight="1"

                                android:textSize="@dimen/textSize18"
                                android:fontFamily="sans-serif"
                                android:textColor="@color/light_gray"
                                android:gravity="right"
                                />
                            <com.vayortricks.vtproductscanner.CustomView.CustomTextView
                                android:id="@+id/txt_formatName"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="cvcvcv"
                                android:fontFamily="sans-serif"
                                android:layout_weight="1"
                                android:textSize="@dimen/textSize18"
                                android:layout_marginLeft="@dimen/margin10"
                                android:textColor="@color/light_gray"
                                android:gravity="left"
                                />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="8dp"
                            >
                            <com.vayortricks.vtproductscanner.CustomView.CustomTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Content :"
                                android:textStyle="bold"
                                android:layout_weight="1"
                                android:textSize="@dimen/textSize18"
                                android:fontFamily="sans-serif"
                                android:textColor="@color/light_gray"
                                android:gravity="right"
                                />
                            <com.vayortricks.vtproductscanner.CustomView.CustomTextView
                                android:id="@+id/txt_scanText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="cvcvcv"
                                android:layout_marginLeft="@dimen/margin10"
                                android:layout_weight="1"
                                android:textSize="@dimen/textSize18"
                                android:fontFamily="sans-serif"
                                android:textColor="@color/light_gray"
                                android:gravity="left"
                                />

                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="8dp"
                            android:paddingBottom="@dimen/margin25"
                            >
                            <com.vayortricks.vtproductscanner.CustomView.CustomTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Date :"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:textStyle="bold"
                                android:fontFamily="sans-serif"
                                android:textSize="@dimen/textSize16"
                                android:textColor="@color/light_gray"
                                />
                            <com.vayortricks.vtproductscanner.CustomView.CustomTextView
                                android:id="@+id/txt_scanDate"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="ddff"
                                android:layout_marginLeft="@dimen/margin10"
                                android:layout_weight="1"
                                android:gravity="left"
                                android:textSize="@dimen/textSize16"
                                android:fontFamily="sans-serif"
                                android:textColor="@color/light_gray"
                                />

                        </LinearLayout>

                    </LinearLayout>
                </ScrollView>

            </androidx.cardview.widget.CardView>

        </LinearLayout>

    </FrameLayout>



</RelativeLayout>
